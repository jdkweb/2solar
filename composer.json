{
    "name": "jdkweb/2solar",
    "description": "Handle specific requests from 2solar server",
    "require": {
        "phpmailer/phpmailer": "^6.6",
        "laminas/laminas-log": "^2.15",
        "swiftmailer/swiftmailer": "^6.3",
        "symfony/mailer": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "TwoSolar\\": "src/",
            "TwoSolarTest\\": "Test/"
        }
    },
    "authors": [
        {
            "name": "jasper",
            "email": "jasper@jdkweb.nl"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "helmich/phpunit-json-assert": "^3.4"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        },
        "platform": {
            "php": "8.0.28"
        }
    }
}
